<UserControl x:Class="QLabel.Windows.Misc_Panel.MiscPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QLabel.Windows.Misc_Panel"
             xmlns:subpanel="clr-namespace:QLabel.Windows.Misc_Panel.Sub_Panels"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="300">
	<TabControl>
		<TabItem Header="Anno">
			<subpanel:AnnoPropertiesPanel
				Margin="10"
				x:Name="anno_properties_panel"
				/>
		</TabItem>
		<TabItem Header="Labels" HorizontalAlignment="Left" Width="45">
			<subpanel:LabelsPanel
				Margin="10"
				x:Name="labels_panel"
				/>
		</TabItem>
		<TabItem Header="Props">
			<subpanel:ImagePropertiesPanel
				Margin="10"
				x:Name="image_properties_panel"
				/>
		</TabItem>
	</TabControl>
</UserControl>
