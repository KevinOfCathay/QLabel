<UserControl x:Class="QLabel.Windows.Misc_Panel.Sub_Panels.ImagePropertiesPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
		   xmlns:option="clr-namespace:QLabel.Custom_Control.Small_Tools.Option_Box" 
             xmlns:local="clr-namespace:QLabel.Windows.Misc_Panel.Sub_Panels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="300">
	<StackPanel>
		<option:OptionTextblock x:Name="file_path" MainText="File Path" SubText="Absolute file path"/>
		<option:OptionTextblock x:Name="file_name" MainText="File Name" SubText="File name"/>
		<option:OptionTextblock x:Name="dimension" MainText="Dimension" SubText="Image dimension"/>
		<option:OptionTextblock x:Name="file_size" MainText="File Size" SubText="File size"/>
	</StackPanel>
</UserControl>
