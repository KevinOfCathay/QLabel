<UserControl x:Class="QLabel.Windows.Misc_Panel.Sub_Panels.ImagePropertiesPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QLabel.Windows.Misc_Panel.Sub_Panels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="300">
	<StackPanel>
		<Expander Header="Properties" IsExpanded="True">
			<StackPanel Margin="20,4,0,0">
				<TextBlock FontSize="10.5">
							<Run>File Path:</Run>
							<Run Name="file_path"></Run>
				</TextBlock>
				<TextBlock FontSize="10.5">
							<Run>Dimension:</Run>
							<Run Name="dimension"></Run>
				</TextBlock>
				<TextBlock FontSize="10.5">
							<Run>File Size:</Run>
							<Run Name="file_size"></Run>
				</TextBlock>
			</StackPanel>
		</Expander>
	</StackPanel>
</UserControl>
