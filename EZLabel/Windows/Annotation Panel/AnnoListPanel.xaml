<UserControl x:Class="EZLabel.Windows.Annotation_List.AnnoListPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EZLabel.Windows.Annotation_List"
             xmlns:annolist ="clr-namespace:EZLabel.Windows.Annotation_List.Anno_List"
		   mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="250">
	<TabControl>
		<TabItem Header="Annos">
			<annolist:AnnoList/>
		</TabItem>
		<TabItem Header="Labels">
			<annolist:LabelList/>
		</TabItem>
	</TabControl>
</UserControl>
