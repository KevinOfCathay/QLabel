<UserControl x:Class="QLabel.Windows.Annotation_Panel.AnnoListPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QLabel.Windows.Annotation_Panel"
             xmlns:anno ="clr-namespace:QLabel.Windows.Annotation_Panel.Sub_Panels"
		   mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="250">
	<TabControl>
		<TabItem Header="List">
			<anno:AnnoList x:Name="annolist"/>
		</TabItem>
		<TabItem Header="Tree">
			<anno:AnnoTree x:Name="annotree"/>
		</TabItem>
	</TabControl>
</UserControl>
