<UserControl x:Class="EZLabel.Windows.Annotation_List.Anno_List.AnnoList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EZLabel.Windows.Annotation_List.Anno_List"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<ListView 
		ScrollViewer.VerticalScrollBarVisibility="Auto"
		ScrollViewer.HorizontalScrollBarVisibility="Visible">

		<ListView.View>
			<GridView>
				<!-- 索引 -->
				<GridViewColumn Header="Index">
				</GridViewColumn>

				<!-- 标记的类型 -->
				<GridViewColumn Header="Type" >
				</GridViewColumn>

				<!-- 标记的类别 -->
				<GridViewColumn Header="Class" >
				</GridViewColumn>

				<!-- 标记的标签 -->
				<GridViewColumn Header="Label" >
				</GridViewColumn>
				
			</GridView>
		</ListView.View>
	</ListView>
</UserControl>
