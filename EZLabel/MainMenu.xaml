<UserControl x:Class="QLabel.MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QLabel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<Menu Height="20">
		<MenuItem Header="_File" >
			<MenuItem Header="_New" Click="New_Click"/>
			<MenuItem Header="_Open Saved" />
			<MenuItem Header="_Save Project" />
			<Separator />
			<MenuItem Header="_Test"/>
			<MenuItem Header="_Close" />
		</MenuItem>
		<MenuItem Header="_Image">
			<MenuItem Header="_Adjustments">
				<MenuItem Header="Brightness/Contrast" />
			</MenuItem>
			<MenuItem Header="_Image Size"/>
			<MenuItem Header="_Crop"/>
		</MenuItem>
	</Menu>
</UserControl>
