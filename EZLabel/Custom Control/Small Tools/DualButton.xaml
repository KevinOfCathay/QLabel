<UserControl 
	x:Name="dualbutton"
	x:Class="QLabel.Custom_Control.Small_Tools.DualButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QLabel.Custom_Control.Small_Tools"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
	<StackPanel 
			Orientation="Horizontal" 
			HorizontalAlignment="Right">
		<Button 
			Name="a" Style="{StaticResource NiceButton}" 
		    Background="{StaticResource B600SCB}"
			Content="{Binding TextA, ElementName=dualbutton}"/>

		<Button 
			Name="b"
			Style="{StaticResource NiceButton}" Background="{StaticResource G600SCB}"
			Content="{Binding TextB, ElementName=dualbutton}"/>
	</StackPanel>
</UserControl>
